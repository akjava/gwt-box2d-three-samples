<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="threebox2d">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="org.jbox2d.GwtBox2D" />
	<inherits name="com.akjava.gwt.lib.Commons"/>
	<inherits name="com.akjava.gwt.three.Three"/>
	<inherits name="com.akjava.gwt.stats.Stats"/>
		<inherits name='com.github.gwtbootstrap.Bootstrap' />
<set-property name="bootstrap.responsiveDesign" value="true"/>
	<source path="client" />
	<entry-point class="com.akjava.gwt.threebox2d.client.Main" />
	
		<!--
	 <set-property name="user.agent" value="safari"/>
	  for test  -->
</module>